<span class='pull-right'>
  {{#if session.isAuthenticated}}
    <span {{action 'logout'}}>Logout {{session.currentUser.displayName}}</span>
  {{else}}
    {{link-to 'Login' 'login'}}
  {{/if}}
</span>
<hr>
<div class="flash-messages">
  {{#each flashMessages.queue as |flash|}}
    {{flash-message flash=flash}}
  {{/each}}
</div>
{{outlet}}
